                            <div class="single--courses d-flex align-items-center">
                                <div class="thumb">
                                    <img src="/public/home/img/bg-img/yml.jpg" alt="">
                                </div>
                                <div class="content">
                                    <h6><%= course.cuppon %></h6>
                                </div>
                            </div>

                           
                        </div>
                    </div>
                </div>
