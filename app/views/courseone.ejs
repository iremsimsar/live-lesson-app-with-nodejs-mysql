    <div class="breadcumb-area">
        <!-- Breadcumb -->
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="/">Home</a></li>
                <li class="breadcrumb-item"><a href="/Kurslar">Kurslar</a></li>
                <li class="breadcrumb-item"><a href="/Kurslar-<%=course.id%>"><%= course.name %></a></li>
                <div class="col-12 col-md-6 col-lg-4">
                    <div class="single-popular-course mb-100 wow fadeInUp" data-wow-delay="250ms">

            </ol>
        </nav>
    </div>
    <!-- ##### Breadcumb Area End ##### -->

    <!-- ##### Single Course Intro Start ##### -->
    <div class="single-course-intro d-flex align-items-center justify-content-center" style="background-image: url(public/assets/images/resim.jpeg);">
        <!-- Content -->
        <div class="single-course-intro-content text-center">
            <!-- Ratings -->
            <div class="ratings">
                <i class="fa fa-star" aria-hidden="true"></i>
                <i class="fa fa-star" aria-hidden="true"></i>
                <i class="fa fa-star" aria-hidden="true"></i>
                <i class="fa fa-star" aria-hidden="true"></i>
                <i class="fa fa-star" aria-hidden="true"></i>
            </div>
            <h3> 
                 <%= course.name %>
            </h3>
            <div class="meta d-flex align-items-center justify-content-center">
                <a href="#"><%= course.category %></a>
            </div>
            <div class="meta d-flex align-items-center justify-content-center">
                <a href="#"><%= course.teacher %></a>
            </div>
            <div class="price"><%= course.price %> $</div>
        </div>
    </div>
    <!-- ##### Single Course Intro End ##### -->

