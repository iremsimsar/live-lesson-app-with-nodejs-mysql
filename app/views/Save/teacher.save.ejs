<%- contentFor('HeaderCss') %>
<!-- perticular page css -->
  

<%- contentFor('body') %>
<div class="content">
                    <div class="container-fluid">

                        <div class="row">
                            <div class="col-sm-12">
                                <div class="page-title-box">
                                    <h4 class="page-title">Kayıt</h4>
                                    <ol class="breadcrumb">
                                        <li class="breadcrumb-item"><a href="javascript:void(0);">Admin</a></li>
                                        <li class="breadcrumb-item"><a href="javascript:void(0);">Kullanıcı Kaydı</a></li>
                                        <li class="breadcrumb-item"><a href="javascript:void(0);">Öğretmen Kayıt</a></li>
                                    </ol>
                               <% include ../Partials/TinyCharts %>
                                </div>
                            </div>
                        </div>
                        <!-- end row -->

                        <div class="row">
                            <div class="col-12">
                                <div class="card m-b-20">
                                <form action="admin-teacher-signup" method="POST">
                                    
                                    <div class="card-body">
      
                                        <h4 class="mt-0 header-title">Öğretmen Kaydı</h4>
                                        
                                        <div class="form-group row">
                                            <label for="example-text-input" class="col-sm-2 col-form-label">Name</label>
                                            <div class="col-sm-10">
                                                <input class="form-control" type="text" name="name" placeholder="İrem" id="example-text-input">
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label for="example-search-input" class="col-sm-2 col-form-label">Surname</label>
                                            <div class="col-sm-10">
                                                <input class="form-control" type="search" name="surname" placeholder="SİMSAR" id="example-search-input">
                                            </div>
                                        </div>
                                         <div class="form-group row">
                                            <label for="example-search-input" class="col-sm-2 col-form-label">iremsmsr</label>
                                            <div class="col-sm-10">
                                                <input class="form-control" type="search" name="username" placeholder="irmsmsr" id="example-search-input">
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label for="example-email-input" class="col-sm-2 col-form-label">Email</label>
                                            <div class="col-sm-10">
                                                <input class="form-control" type="email" name="email" placeholder="iremsimsar46@example.com" id="example-email-input">
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label for="example-url-input" class="col-sm-2 col-form-label">Password</label>
                                            <div class="col-sm-10">
                                                <input class="form-control" type="password" name="password" id="example-url-input">
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label for="example-tel-input" class="col-sm-2 col-form-label">Telephone</label>
                                            <div class="col-sm-10">
                                                <input class="form-control" type="tel" name="phone" placeholder="1-(555)-555-5555" id="example-tel-input">
                                            </div>
                                        </div>
                                       
                                        <div class="form-group row">
                                            <label for="example-date-input" class="col-sm-2 col-form-label">Date</label>
                                            <div class="col-sm-10">
                                                <input class="form-control" type="date" name="createdAt" placeholder="2011-08-19" id="example-date-input">
                                            </div>
                                        </div>
                                        
                                      
                                        <div class="form-group row">
                                            <label class="col-sm-2 col-form-label">Gender</label>
                                            <div class="col-sm-10">
                                                <select class="form-control">
                                                    <option> Kadın</option>
                                                    <option>Erkek</option>
                                                </select>
                                            </div>
                                        </div>

                                    </div>
                                </form>
                                <div class="button-items">
                                            <button type="submit" class="btn btn-success waves-effect waves-light">Success</button>
                                </div>
                            </div> <!-- end col -->
                        </div> <!-- end row -->
        

                    </div> <!-- container-fluid -->

                </div> <!-- content -->

<%- contentFor('FooterJs') %>